<?xml version="1.0"?>
<translations>

  <translation>
    <name>highway=trunk</name>
    <description>-</description>
    <match type="way">
      <tag k="Mode" v="highway" />
      <tag k="Type" v="trunk" />
      <tag k="Name" v="(.*)" match_id="name"/>
    </match>
    <output>
      <copy-all/>
      <tag k="highway" v="trunk" />
      <!--<tag k="name" v="Main road [highway=trunk]" />-->
      <tag k="name" from_match="name" v="{1}"/>
    </output>
  </translation>
 
