<?xml version="1.0"?>
<translations>
 
  <translation>
    <name>highway=primary</name>
    <description>-</description>
    <match type="way">
      <tag k="Mode" v="highway" />
      <tag k="Type" v="primary" />
    </match>
    <output>
      <copy-all/>
      <tag k="highway" v="primary" />
      <tag k="name" v="Main road (unnamed) [highway=primary]" />
    </output>
  </translation>

